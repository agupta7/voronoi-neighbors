{
  "name": "voronoi-frontend",
  "version": "1.0.0",
  "description": "A demo of the concept behind the IEEE paper Spatial Query Integrity with Voronoi Neighbors",
  "homepage": "https://github.com/agupta7/voronoi-neighbors/tree/master/frontend",
  "main": "express-spa.js",
  "scripts": {
    "start": "npm-run-all clean build serveprod",
    "test": "npm-run-all clean buildDebug karma",
    "build": "webpack --bail --progress --profile --env.production",
    "karma": "karma start test/karma.conf.js",
    "debug": "npm-run-all --parallel watch servedebug",
	 "debugTest": "npm-run-all buildTest servetest",
    "buildTest": "webpack --bail --progress --profile --env.test",
	 "buildDebug": "webpack --progress --verbose --env.dubug",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "clean": "rimraf .bin",
    "servedebug": "nodejs express-spa.js debug",
    "serveprod": "nodejs express-spa.js dist",
	 "servetest": "nodejs express-spa.js testBuild",
    "watch": "webpack --watch --progress --verbose --env.debug"
  },
  "ports": {
    "dev": 8888,
    "debug": 8080,
    "dist": 8080,
    "testBuild": 8889,
    "backend": 8081
  },
  "author": {
    "name": "Anuj Gupta",
    "email": "azg0076@tigermail.auburn.edu"
  },
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.5",
    "angular-route": "^1.6.5",
    "express": "^4.15.3",
    "http-proxy": "^1.16.2",
    "jsrsasign": "^8.0.9",
    "milligram": "^1.3.0",
    "ng-dialog": "^1.4.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.5",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.8.0",
    "jasmine": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
